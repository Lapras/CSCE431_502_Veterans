<h1>Membership Requests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td>
          <%= button_to "Approve", approve_admin_membership_request_path(user), method: :patch %>
          <%= button_to "Deny", deny_admin_membership_request_path(user), method: :patch %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
